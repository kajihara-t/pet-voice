{
  "name": "peek-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "npx expo lint .",
    "lint:fix": "npx expo lint . --fix",
    "depcruise:html": "npx depcruise . --config .dependency-cruiser.js --output-type html > docs/dependencies/dependency-graph.html",
    "depcruise:svg": "npx depcruise . --config .dependency-cruiser.js --output-type dot | dot -T svg > docs/dependencies/dependency-graph.svg",
    "depcruise:components": "npx depcruise . --config .dependency-cruiser.js --include-only \"^components\" --output-type dot | dot -T svg > docs/dependencies/components-graph.svg",
    "depcruise:features": "npx depcruise . --config .dependency-cruiser.js --include-only \"^features\" --output-type dot | dot -T svg > docs/dependencies/features-graph.svg"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "color": "^4.2.3",
    "expo": "~52.0.20",
    "expo-camera": "~16.0.10",
    "expo-dev-client": "~5.0.7",
    "expo-font": "~13.0.2",
    "expo-image": "~2.0.3",
    "expo-image-picker": "~16.0.3",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~7.0.3",
    "expo-media-library": "~17.0.4",
    "expo-notifications": "~0.29.11",
    "expo-router": "~4.0.15",
    "expo-sharing": "~13.0.0",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "lottie-react-native": "7.1.0",
    "lucide-react-native": "^0.469.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-async-storage": "^0.0.1",
    "react-native-confetti": "^0.1.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-onboarding-swiper": "^1.3.0",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-view-shot": "~4.0.3",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/color": "^4.2.0",
    "@types/react": "~18.3.12",
    "@types/react-native-confetti": "^0.1.4",
    "@types/react-native-onboarding-swiper": "^1.1.9",
    "dependency-cruiser": "^16.8.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "18.3.1",
    "typedoc": "^0.27.5",
    "typescript": "~5.3.3"
  },
  "private": true
}
